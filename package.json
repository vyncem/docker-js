{
  "name": "docker-js-template",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node_modules/.bin/nodemon --config nodemon.config.json ./index.js",
    "dev:debugger": "node_modules/.bin/nodemon --config nodemon.config.json --inspect-brk=0.0.0.0:9229 ./index.js",
    "test": "./node_modules/.bin/jest --config jest.config.js",
    "test:watch": "./node_modules/.bin/jest --config jest.config.js --watchAll",
    "test:coverage": "./node_modules/.bin/jest --config jest.config.js --coverage",
    "test:watch:coverage": "./node_modules/.bin/jest --config jest.config.js --watchAll --coverage",
    "lint": "./node_modules/.bin/eslint **/*.js",
    "pretty": "./node_modules/.bin/prettier --config ./prettier.config.js --write \"src/**/*.js\""
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.0"
  }
}
